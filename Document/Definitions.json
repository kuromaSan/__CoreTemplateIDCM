{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "User": {
            "type": "object",
            "properties": {
            "id": { "type": "integer" },
            "name": { "type": "string" }
            },
            "required": ["id", "name"]
        },
        "Product": {
            "type": "object",
            "properties": {
            "productId": { "type": "integer" },
            "title": { "type": "string" }
            },
            "required": ["productId", "title"]
        }
    }
}